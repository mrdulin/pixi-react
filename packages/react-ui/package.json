{
  "name": "@pixi/react-ui",
  "version": "7.0.2",
  "description": "React bindings for Pixi UI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixi-react.git",
    "directory": "packages/react-ui"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "module.js",
  "jsnext:main": "module.js",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./module.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "index.d.ts",
    "index.js",
    "module.js"
  ],
  "scripts": {
    "clean": "rimraf dist index.d.ts",
    "prebuild": "npm run clean",
    "xbuild": "npm run build:types && npm run build:dev && npm run build:prod",
    "xbuild:types": "tsc -p tsconfig.types.json",
    "xbuild:dev": "cross-env NODE_ENV=development rollup -c",
    "xbuild:prod": "cross-env NODE_ENV=production rollup -c",
    "xtest": "jest --silent",
    "xtest:ci": "jest --silent --runInBand",
    "test:types": "tsc --noEmit",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@pixi/ui": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@pixi/react": "*",
    "@pixi/react-types": "*",
    "@storybook/addon-essentials": "7.0.0-beta.64",
    "@storybook/addon-interactions": "7.0.0-beta.64",
    "@storybook/addon-links": "7.0.0-beta.64",
    "@storybook/addon-storysource": "^6.5.14",
    "@storybook/blocks": "^7.0.0-beta.64",
    "@storybook/react": "^7.0.0-beta.64",
    "@storybook/react-webpack5": "^7.0.0-beta.64",
    "@storybook/testing-library": "^0.0.13",
    "babel-loader": "^8.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "7.0.0-beta.64"
  },
  "peerDependencies": {
    "@pixi/app": ">=6.0.0",
    "@pixi/constants": ">=6.0.0",
    "@pixi/core": ">=6.0.0",
    "@pixi/display": ">=6.0.0",
    "@pixi/graphics": ">=6.0.0",
    "@pixi/math": ">=6.0.0",
    "@pixi/mesh-extras": ">=6.0.0",
    "@pixi/particle-container": ">=6.0.0",
    "@pixi/react": ">=7.0.0",
    "@pixi/sprite": ">=6.0.0",
    "@pixi/sprite-animated": ">=6.0.0",
    "@pixi/sprite-tiling": ">=6.0.0",
    "@pixi/text": ">=6.0.0",
    "@pixi/text-bitmap": ">=6.0.0",
    "@pixi/ticker": ">=6.0.0",
    "prop-types": ">=15.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
