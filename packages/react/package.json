{
  "name": "@pixi/react",
  "version": "7.1.1",
  "description": "Write PixiJS applications using React declarative style.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixi-react.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "index.js",
  "module": "module.js",
  "jsnext:main": "module.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./module.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "index.js",
    "module.js"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:versions && npm run build:types && npm run build:dev && npm run build:prod",
    "build:types": "tsc -p tsconfig.types.json",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "build:versions": "./scripts/getVersions.mjs",
    "postbuild": "./scripts/fixDeclarationReference.mjs",
    "clean": "rimraf ./dist",
    "test": "jest --silent",
    "test:ci": "jest --silent --runInBand",
    "test:types": "tsc --noEmit",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "lodash.isnil": "4.0.0",
    "lodash.times": "4.3.2",
    "react-reconciler": ">=0.29.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/lodash.isnil": "^4.0.7",
    "canvas": "^2.11.0",
    "jest-webgl-canvas-mock": "^0.2.3",
    "react-test-renderer": "18.2.0",
    "rollup-plugin-copy": "^3.4.0"
  },
  "peerDependencies": {
    "pixi.js": ">=8.0.0-0",
    "prop-types": "^15.8.1",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "6d5b98bc6d7eafd1b0016e60a725b2b6c9f46a33"
}
