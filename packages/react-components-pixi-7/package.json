{
  "name": "@pixi/react-components-pixi-v7",
  "private": true,
  "description": "PixiJS V7 components for the @pixi/react library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixi-react.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./module.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "main": "index.js",
  "module": "module.js",
  "jsnext:main": "module.js",
  "typings": "./index.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "clean": "rimraf ../react-components/v7/dist",
    "test": "jest --silent",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@pixi/react-invariant": "*",
    "lodash.isnil": "4.0.0",
    "performance-now": "2.1.0",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@pixi/app": "^7.0.0",
    "@pixi/constants": "^7.0.0",
    "@pixi/core": "^7.0.0",
    "@pixi/display": "^7.0.0",
    "@pixi/events": "^7.1.1",
    "@pixi/graphics": "^7.0.0",
    "@pixi/math": "^7.0.0",
    "@pixi/mesh-extras": "^7.0.0",
    "@pixi/particle-container": "^7.0.0",
    "@pixi/react-fiber": "*",
    "@pixi/react-invariant": "*",
    "@pixi/react-modular": "*",
    "@pixi/sprite": "^7.0.0",
    "@pixi/sprite-animated": "^7.0.0",
    "@pixi/sprite-tiling": "^7.0.0",
    "@pixi/text": "^7.0.0",
    "@pixi/text-bitmap": "^7.0.0",
    "@pixi/ticker": "^7.0.0",
    "@testing-library/react": "^13.4.0",
    "jest-webgl-canvas-mock": "^0.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0"
  },
  "peerDependencies": {
    "@pixi/app": ">=6.0.0",
    "@pixi/constants": ">=6.0.0",
    "@pixi/core": ">=6.0.0",
    "@pixi/display": ">=6.0.0",
    "@pixi/graphics": ">=6.0.0",
    "@pixi/math": ">=6.0.0",
    "@pixi/mesh-extras": ">=6.0.0",
    "@pixi/particle-container": ">=6.0.0",
    "@pixi/sprite": ">=6.0.0",
    "@pixi/sprite-animated": ">=6.0.0",
    "@pixi/sprite-tiling": ">=6.0.0",
    "@pixi/text": ">=6.0.0",
    "@pixi/text-bitmap": ">=6.0.0",
    "@pixi/ticker": ">=6.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
