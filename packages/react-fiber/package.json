{
  "name": "@pixi/react-fiber",
  "version": "7.0.2",
  "description": "Configurable React Reconciler for PixiJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixi-react.git",
    "directory": "packages/react-fiber-react-18"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./v18/index.d.ts",
        "default": "./v18/module.js"
      },
      "require": {
        "types": "./v18/index.d.ts",
        "default": "./v18/index.js"
      }
    }
  },
  "main": "v18/index.js",
  "module": "v18/module.js",
  "jsnext:main": "v18/module.js",
  "typings": "./v18/index.d.ts",
  "files": [
    "v18/",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@pixi/react-invariant": "*"
  },
  "devDependencies": {
    "@pixi/react-fiber-react-18": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
