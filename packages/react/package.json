{
  "name": "@pixi/react",
  "version": "7.0.2",
  "description": "Write PixiJS applications using React declarative style.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixi-react.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./module.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "main": "index.js",
  "module": "module.js",
  "jsnext:main": "module.js",
  "typings": "./index.d.ts",
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "index.d.ts",
    "index.js",
    "module.js"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "clean": "rimraf ./dist",
    "test": "jest --silent",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@pixi/react-components": "*",
    "@pixi/react-fiber": "*",
    "@pixi/react-invariant": "*",
    "@pixi/react-modular": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@pixi/app": "^7.0.0",
    "@pixi/constants": "^7.0.0",
    "@pixi/core": "^7.0.0",
    "@pixi/display": "^7.0.0",
    "@pixi/events": "^7.1.1",
    "@pixi/graphics": "^7.0.0",
    "@pixi/math": "^7.0.0",
    "@pixi/mesh-extras": "^7.0.0",
    "@pixi/particle-container": "^7.0.0",
    "@pixi/sprite": "^7.0.0",
    "@pixi/sprite-animated": "^7.0.0",
    "@pixi/sprite-tiling": "^7.0.0",
    "@pixi/text": "^7.0.0",
    "@pixi/text-bitmap": "^7.0.0",
    "@pixi/ticker": "^7.0.0",
    "@testing-library/react": "^13.4.0",
    "jest-webgl-canvas-mock": "^0.2.3",
    "react-test-renderer": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
